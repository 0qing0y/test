.contact-container {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    height: 100vh;
  }
  
  .contact-text {
    margin-bottom: 20px;
    font-size: 16px;
    color: rgb(51, 51, 51);
    text-align: center;
  }
  button {
    width: 30%;
    margin: 10px 0;
    background-color: #1aad19;
    color: white;
    border: none;
    border-radius: 4px;
  }