
/* 样式代码根据需要自定义 */
.edit-phone-container {
    padding: 20px;
  }
  .title {
    font-size: 24px;
    margin-bottom: 20px;
  }
  input {
    width: 95%;
    margin: 10px 0;
    padding: 10px;
    border: 1px solid #ccc;
    border-radius: 4px;
  }
  button {
    width: 100%;
    margin: 10px 0;
    padding: 10px;
    background-color: #1aad19;
    color: white;
    border: none;
    border-radius: 4px;
  }